{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "total": {
      "type": "float"
    },
    "paymentMethod": {
      "type": "enumeration",
      "enum": [
        "cod",
        "bkash",
        "nagod"
      ]
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "placed",
        "shipped",
        "delivered"
      ]
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product"
    }
  }
}
